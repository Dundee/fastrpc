#ifndef FRPCVERSION_H_
#define FRPCVERSION_H_

/** FastRPC library major version (libtool's CURRENT version component).
 */
#define FASTRPC_MAJOR @FASTRPC_MAJOR@

/** FastRPC library minor version (libtool's REVISION version component).
 */
#define FASTRPC_MINOR @FASTRPC_MINOR@

/** FastRPC library version.
 */
#define FASTRPC_VERSION "@FASTRPC_MAJOR@.@FASTRPC_MINOR@"

#endif // FRPCVERSION_H_
